<%= link_to @subject.category.name, category_path(@subject.category) %> > <%= @subject.name %> <br><br>

<%= link_to "Edit this Subject.", edit_subject_path %> <br>

<%= link_to "Make a new Resource!", { controller: "resources", action: "new", category_id: @subject.category.id, subject_id: @subject.id} %>



<ul>
  <%= @subject.resources.each do |resource| %>

    <li><%= render partial: "application/index", locals: { object: resource } %></li>
  <% end %>
</ul>