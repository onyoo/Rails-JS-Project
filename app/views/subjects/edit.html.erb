
<title><%= title("Edit " + @subject.name) %></title>

<%= render partial: "application/edit_object_instructions", locals: { object: @subject } %>

<div class="form">

  <%= form_for [@subject.category, @subject], :url => edit_category_subject_path(@subject.category, @subject) do |f| %>

